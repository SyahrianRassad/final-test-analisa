         <input\r\n                    type=\"file\"\r\n                    className=\"file-input\"\r\n                    onChange={loadImage}\r\n                  />\r\n                  <span className=\"file-cta\">\r\n                    <span className=\"file-label\">Choose a file...</span>\r\n                  </span>\r\n                </label>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          {preview ? (\r\n            <figure className=\"image is-128x128\">\r\n              <img src={preview} alt=\"Preview Image\" />\r\n            </figure>\r\n          ) : (\r\n            \"\"\r\n          )}\r\n\r\n          <div className=\"field\">\r\n            <div className=\"control\">\r\n              <button type=\"submit\" className=\"button is-success\">\r\n                Save\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default AddProduct\r\n","C:\\Users\\asus\\Documents\\OJT\\analisa 99% usahaku\\project\\FE-dummy-99\\src\\components\\FormRegister.jsx",[],[],"C:\\Users\\asus\\Documents\\OJT\\analisa 99% usahaku\\project\\FE-dummy-99\\src\\components\\Footer.jsx",[],[],"C:\\Users\\asus\\Documents\\OJT\\analisa 99% usahaku\\project\\FE-dummy-99\\src\\components\\Navbar.jsx",["145","146"],[],"import React,{useState} from 'react'\r\nimport PopUpLogin from './PopUpLogin'\r\nimport { Link } from 'react-router-dom';\r\n\r\nconst Navbar = () => {\r\n  const [buttonPopUp, setButtonPopUp] = useState(false);\r\n\r\n  return (\r\n    <div>\r\n    <nav className=\"navbar has-shadow\" role=\"navigation\" aria-label=\"main navigation\">\r\n      <div className=\"navbar-brand\">\r\n      <figure class=\"image is-96x96\">\r\n            <img src=\"https://99usahaku.com/logo144.png\"/>\r\n        </figure>\r\n    \r\n        <a role=\"button\" className=\"navbar-burger burger\" aria-label=\"menu\" aria-expanded=\"false\" data-target=\"navbarB